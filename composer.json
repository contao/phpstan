{
    "name": "contao/phpstan",
    "type": "phpstan-extension",
    "description": "Contao extension for PHPStan",
    "license": "MIT",
    "authors": [
        {
            "name": "David Greminger",
            "homepage": "https://github.com/bytehead"
        }
    ],
    "require": {
        "php": "^7.1",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "symfony/debug": "^4.3",
        "symfony/monolog-bridge": "^4.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/templating": "^4.3"
    },
    "require-dev": {
        "contao/core-bundle": "^4.8.1",
        "contao/test-case": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "lexik/maintenance-bundle": "^2.1.3",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.5",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/http-client": "^4.3",
        "symfony/var-dumper": "^4.3"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        },
        "symfony": {
            "require": "^4.3"
        }
    },
    "autoload": {
        "psr-4": {
            "Contao\\PhpStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\PhpStan\\Tests\\": "tests/",
            "Contao\\CoreBundle\\Tests\\": "vendor/contao/core-bundle/tests/"
        }
    },
    "support": {
        "issues": "https://github.com/contao/phpstan/issues",
        "source": "https://github.com/contao/phpstan"
    }
}
